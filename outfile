		$(document).ready(function () {
			$('.firstsubscribe-city_wrap').hide();
			
			
			$('#cityul').click(function(){
				if( $("#zoneselect option:selected").val()!="0"){
					$('.skipit').attr('href', 'http://www.snapdeal.com/deals-' + $("#zoneselect :selected").attr("pageUrl"));
				}
			});
			
			$('#subsmodalmobile').ajaxForm({dataType: "json", success: function(data) { 
				var msg=data['message'];
			    var msg1 = "Subscribed Successfully.";
				if(msg == msg1)
				{
					$('#openmodal').hide();
					$('.subscription_thank').show();
					
				}
				$('#subsRes').html(data['message'])
			}});
		});	
	
		
		if($('#nu').html() == "1"){
			 
			 $("#overlay").css("filter","alpha(opacity=80)").show();
			    $("#lightbox").show("fast"); 
			   /* $("#lightbox .close").click(function(){
					$("#overlay").fadeOut("slow");
					 $("#lightbox").fadeOut("slow");
					return false;
					});*/
					
