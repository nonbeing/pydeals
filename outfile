					 $("#lightbox").fadeOut("slow");
					 /*$("#overlay2").css("filter","alpha(opacity=80)").show();
					 $("#thankyou_wrap").show("fast"); */
					return false;
					});	
			    
		}
		if($('#nufd').html().length > 0){			
			$(".subscription").show();
		    $(".subscription .hide a").click(function(){
				$(".subscription").slideUp("slow");
				return false;
				});
